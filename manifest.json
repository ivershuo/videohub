{
  "manifest_version": 2,
  "name" : "Videohub",
  "version" : "0.1",  
  "description" : "Videohub",
  "background": {    
    "scripts": ["siteconf.js", "background.js"]
},
"permissions" : [
    "tabs", "cookies", "*://*/*", "contextMenus"
],
"content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["res/videohub_page.js"],
      "all_frames" : true
    },
    {
      "matches": ["*://*.xunlei.com/*"],
      "js": ["res/xunlei_on_change.js"]      
    }
],
"icons": {
    "16": "res/icons/videohub_16.png", 
    "48": "res/icons/videohub_48.png", 
    "128": "res/icons/videohub_128.png"
},
"options_page": "options.html"
}